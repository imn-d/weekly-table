{
  "name": "react-weekly-table",
  "version": "1.0.3",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "homepage": "https://github.com/imn-d/weekly-table",
  "repository": {
    "type": "git",
    "url": "https://github.com/imn-d/weekly-table.git"
  },
  "keywords": [
    "weekly",
    "scheduler",
    "timerange"
  ],
  "author": "Konev Oleg",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc -b",
    "demo:dev": "webpack --watch --env dev --config webpack.config.js --mode=development",
    "demo:prod": "webpack --config webpack.config.js --mode=production",
    "bundle-report": "webpack-bundle-analyzer --port 4200 public/build/stats.json",
    "format": "prettier --write \"{src,demo,test}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,demo,test}/**/*.ts*\" --fix",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "serve": "serve public"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "css-loader": "^6.5.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "mini-css-extract-plugin": "^2.4.4",
    "normalize.css": "^8.0.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "browserslist": [
    ">0.2%",
    "Firefox ESR",
    "not dead",
    "not op_mini all",
    "not IE <= 11"
  ],
  "jest": {
    "rootDir": ".",
    "testRegex": ".*\\.test\\.tsx?$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>test/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "src",
      "test",
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.(t|j)sx",
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage"
  }
}
